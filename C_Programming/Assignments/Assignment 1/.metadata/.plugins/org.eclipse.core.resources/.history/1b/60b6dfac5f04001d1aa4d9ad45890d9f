#include <stdio.h>

void main ()
{
	float a,b;
	printf ("Enter value of a: ");
	fflush (stdout);
	scanf("%f", &a);
	fflush (stdout);
	printf ("Enter valur of b: ");
	fflush (stdout);
	scanf("%f", &b);

	a = a+b-a;
	printf ("After swapping, value of a = %0.2f \n ",a);
	b = a+b-b;



	//printf ("After swapping, value of a = %0.2f \n ",a);
	//fflush (stdout);
	//printf ("After swapping, value of b = %0.2f ",b);
}


